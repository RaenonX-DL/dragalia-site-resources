[{"conversations": [{"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0055.wav"], "content": "Raaaaaah!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "Graaaahhh!", "isSys": false, "speakerIcon": null, "speakerName": "Fiend", "type": "conversation"}, {"audioPaths": ["vo_chr_120016_story/VO_CHR_120016_STORY_0003.wav"], "content": "Eeeek! I'm scaaared!", "isSys": false, "speakerIcon": "story/chara/120016_01.png", "speakerName": "Child", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0038.wav"], "content": "Bah... There's just no end to them!", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0038.wav"], "content": "They just HAD to come out in droves when we got some kid to protect!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "Raaaaah!", "isSys": false, "speakerIcon": null, "speakerName": "Fiend", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0037.wav"], "content": "C'mon, gimme a break already! I dunno how much longer I can ke—", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0019.wav"], "content": "YEEEEE-HAAAAW!", "isSys": false, "speakerIcon": null, "speakerName": "???", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0011.wav"], "content": "I can't believe there was a damn fight and nobody told me!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0004.wav"], "content": "Aaah ha ha ha! This feels GREAT!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0001.wav"], "content": "Oh, I'm just gettin' warmed up here! LET'S GO, SUCKERS!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0019.wav"], "content": "Aaah ha ha ha!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0055.wav"], "content": "You're the last one, buddy! Get ready to get dyin'!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "She took them out by herself... ALL of them...", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": [], "content": "...Everything's going to be okay now, little girl. This nice lady got rid of all the scary fiends for you.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": [], "content": "Really? They're all gone?", "isSys": false, "speakerIcon": "story/chara/120016_01.png", "speakerName": "Child", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0024.wav"], "content": "Yep! Didn't even break a sweat, if I'm bein' honest...", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_120016_story/VO_CHR_120016_STORY_0017.wav"], "content": "Wow! You're soooo strong!", "isSys": false, "speakerIcon": "story/chara/120016_01.png", "speakerName": "Child", "type": "conversation"}, {"audioPaths": ["vo_chr_120016_story/VO_CHR_120016_STORY_0005.wav"], "content": "Thanks, pretty lady!", "isSys": false, "speakerIcon": "story/chara/120016_01.png", "speakerName": "Child", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0006.wav"], "content": "Huh? ...M-me? Pretty?", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_120016_story/VO_CHR_120016_STORY_0032.wav"], "content": "Yup! You sure are!", "isSys": false, "speakerIcon": "story/chara/120016_01.png", "speakerName": "Child", "type": "conversation"}, {"audioPaths": [], "content": "Wh... Wha... Whaaaa...", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "Er, maybe you misheard. The girl only said that you were pretty.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": [], "content": "Graaaaaah!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0056.wav"], "content": "Whoooa!", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0038.wav"], "content": "Did YOU just call me pretty?!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0025.wav"], "content": "I don't need that kind of flattery!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0003.wav"], "content": "That's it! You're goin' down! Pick up your sword, chump!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "Wait, WHAT?! I don't want to fight you! Plus, I'm already exhausted from fighting all those fiends, so mayb—", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0002.wav"], "content": "Hey, you started this fight, bro. I'm just finishing it.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0065.wav"], "content": "And I'm not gonna let it go!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0029.wav"], "content": "I've got a score to settle with you, and it WILL be settled!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0007.wav"], "content": "Look, I really think there's been some kind of horrible misunderst—", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0002.wav"], "content": "Too late for excuses now, buddy! Hurry up and get your stamina back!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0076.wav"], "content": "Because until we settle this, I'm comin' with you!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0065.wav"], "content": "I can't BELIEVE you had the gall to try and sweet-talk me like that!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}], "id": 110023011, "title": "A Score to Settle (110023011)"}, {"conversations": [{"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0004.wav"], "content": "Oh, you're mine now! Yeeee-haw!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0001.wav"], "content": "Hey! Get over here!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0019.wav"], "content": "Who else wants a piece of Johanna?!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0018.wav"], "content": "...She's at it again, huh?", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0020.wav"], "content": "She really does love to fight.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0008.wav"], "content": "Maybe those fiends told her she was pretty or somethin'. She REALLY don't seem to like that.", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "It's not often you find someone scarier  when complimented than when fighting.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0008.wav"], "content": "I wonder why she's like that?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0008.wav"], "content": "Perhaps she's simply not used to being complimented on her looks?", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": [], "content": "Although I think ＂cute＂ might be a more appropriate word for her than ＂pretty.＂", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0011.wav"], "content": "The way she shifts from one mood to another gives her an almost childlike look.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0030.wav"], "content": "Hmm. Yeah, maybe.", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0012.wav"], "content": "I think she's got a kind of an athletic- beauty thing goin' on.", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "She looks nice even though she don't really seem to worry about it.", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0004.wav"], "content": "Honestly, she almost looks like some kinda rambunctious kid.", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0008.wav"], "content": "I guess I see what you're saying.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": [], "content": "Still, every so often, her expression...", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0012.wav"], "content": "Well, it has this kind of noble elegance to it. I'm not quite sure how to describe it better than that.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0042.wav"], "content": "Noble? A violent woman like her?", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "So ya think she's a noblewoman who got raised by wolves or somethin'?", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0008.wav"], "content": "I'm not sure I subscribe to that idea.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0073.wav"], "content": "Or maybe she's a wild animal that got turned into a human by some wizard!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0002.wav"], "content": "DID YOU JUST CALL ME A WILD ANIMAL?!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0006.wav"], "content": "Owwwww!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0025.wav"], "content": "I wasn't raised by wolves, and I would never let some stupid wizard cast a spell on me!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "You, uh...heard all that, did you?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0011.wav"], "content": "You wanna know the score? Fine.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "I'm from a famous family of knights, and I'm my father's only daughter.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "I was made to fight from the time I could hold a weapon—which is fine by me, because I LOVE IT!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0023.wav"], "content": "But all the stuffy, formal crap like chivalry and bowing and honoring others? No thanks. Not for me.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0008.wav"], "content": "...Uh-huh. So lemme guess: Ya ran away from home to fight, yeah?", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0011.wav"], "content": "You got it.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0019.wav"], "content": "Nope. I'm callin' bullplop on all'a this. Ain't no way YOU'RE the daughter of some famous knight.", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "I don't buy it for a second.", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0042.wav"], "content": "...Say that again, funny man.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0011.wav"], "content": "C'mon! Ya can't be serious!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "Every knight I ever met spends half the day starin' into a mirror, but you look like ya just spent the night sleepin' in a barn. I mean, that armor is FILTHY!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0073.wav"], "content": "Ain't I right, Euden?", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "I'm just going to...back away slowly.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0038.wav"], "content": "DO YOU WANNA DIE, FUNNY MAN?!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0064.wav"], "content": "YEOOOWCH!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0056.wav"], "content": "All right, all right! I'm sorry, yeah?! Yer the daughter of a famous knight! Just stop punchin' me already!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}], "id": 110023012, "title": "The Truth behind the Fury (110023012)"}, {"conversations": [{"audioPaths": [], "content": "......", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "......", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": [], "content": "......", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0008.wav"], "content": "Umm...", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": [], "content": "Might I ask why you're staring so intently at me? I feel like prey looking into the eyes of a hunter.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0006.wav"], "content": "Oh! Um...no reason?", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "It's rude to stare at the best of times, never mind without cause.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0013.wav"], "content": "If there's something you want, you should come out and say it.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0074.wav"], "content": "W-well... Umm...", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "I just wanted to get a better look at your hair and clothes. That's all.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0042.wav"], "content": "Do you have an interest in fashion?", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0006.wav"], "content": "I can't help it!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "Ranzal said knights were supposed to look presentable, and I've never really cared about crap like that. But then...", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0025.wav"], "content": "Rrr! It's all Euden's fault for calling me pretty that one time!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0008.wav"], "content": "You're not making any sense.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0038.wav"], "content": "Ugh.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "Still, since we're here...", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0073.wav"], "content": "I've been wanting to get a look at your face for a while now.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": [], "content": "This seems as good a time as any. Would you mind?", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0032.wav"], "content": "Perhaps I can give you some advice if you're interested in improving your overall appearance.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0012.wav"], "content": "Come. Let me have a look.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0038.wav"], "content": "Blaaargh.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "Normally I'd get aggro about something like this, but since you're a woman...", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0023.wav"], "content": "Oh, fine. Stare at me.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "...Well? What do you think?", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0073.wav"], "content": "Just as I thought. You really are cute.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0032.wav"], "content": "Cute is definitely a more appropriate word for you than pretty.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": [], "content": "Might you make a specific expression for me for a moment?", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": [], "content": "Pretend you're sizing up a fiend. Be still and serious, with your mouth closed.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0008.wav"], "content": "Mmm. That's tricky. Like...this?", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "That's...perfect. Goodness me...", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0012.wav"], "content": "Euden was right.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": [], "content": "Well, this is something of a personal decision, but if you're interested in presenting a different face to the world, you might consider wearing makeup.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0022.wav"], "content": "Y-you think so? I suppose I can try...", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0010.wav"], "content": "Hey, you two. What're you up to?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0056.wav"], "content": "Aaah! Gaaaaah! Stay back!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0056.wav"], "content": "...The heck did I do?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}], "id": 110023013, "title": "Cleo's Beauty Class (110023013)"}, {"conversations": [{"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0055.wav"], "content": "Hyaaaaah!", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0055.wav"], "content": "Raaaaagh!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "......", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0011.wav"], "content": "How did everything go on your end?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0012.wav"], "content": "Perfect! I made short work of 'em. How 'bout you, Johanna?", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "......", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0043.wav"], "content": "Is something wrong, Johanna? You seem a bit unfocused.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0008.wav"], "content": "She's been actin' different all day, if ya ask me.", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "She ain't fightin' like she usually does. And she also ain't doin' all that wild yellin' and laughin' and stuff.", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "I wonder what's up with he— Whoa!", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": [], "content": "Ah!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0074.wav"], "content": "Johanna? Are you...wearing makeup?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0006.wav"], "content": "Wait, seriously?! The Bedhead Berserker's wearin' makeup?!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0011.wav"], "content": "It's subtle, but it really brings out her elegance.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0074.wav"], "content": "Cleo did it! It was Cleo!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "I'm just the victim here!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0025.wav"], "content": "Why don't ya wash it off, then?", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0062.wav"], "content": "Well... Um...", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0073.wav"], "content": "Is it 'cause ya actually kinda like how ya look with it?", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "And since ya never wore it before, yer probably worried all that fiend blood is gonna mess it up, which is why ya ain't fightin' like normal. Am I right?", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0038.wav"], "content": "Gah!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0016.wav"], "content": "So that's what happened. Well, I think it looks nice.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0004.wav"], "content": "In fact, you look even prettier than usu—", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0056.wav"], "content": "Aaaaaaaaaaaah! Stooooooooop!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0038.wav"], "content": "Graah... I knew this would happen...", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": [], "content": "Hey, don't worry about it! It's fine for a crazy violent lady to be pretty and— ARGH!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0056.wav"], "content": "Stop calling me pretty, damn it!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}], "id": 110023014, "title": "Trying Something New (110023014)"}, {"conversations": [{"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0004.wav"], "content": "Aaaah ha ha ha! Now THIS is a good time!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0055.wav"], "content": "Hyaaaaaaah!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0001.wav"], "content": "More! MORE!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0016.wav"], "content": "Well, someone's in fightin' form today.", "isSys": false, "speakerIcon": "story/chara/120007_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0030.wav"], "content": "Ya must have figured out that whole makeup thing, huh?", "isSys": false, "speakerIcon": "story/chara/120007_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0011.wav"], "content": "Actually...", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0019.wav"], "content": "I think it's BECAUSE of the makeup that I'm fighting so well.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0007.wav"], "content": "Wait, really?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": [], "content": "That's kind of unexpected, actually.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0073.wav"], "content": "I've recently noticed that I fight better if I'm wearing makeup.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "So I've been getting Cleo to teach me some of the basics.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0016.wav"], "content": "It's almost like...war paint.", "isSys": false, "speakerIcon": "story/chara/120007_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "There are tribes'a folks around the world who use body paint to make 'em look and feel tougher.", "isSys": false, "speakerIcon": "story/chara/120007_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0030.wav"], "content": "I still have little interest in hair or clothes, but makeup is a different story.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0045.wav"], "content": "So thanks, Euden.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "I owe all of this to what you said.", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0004.wav"], "content": "Now I'm stronger than ever!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": [], "content": "Well, I still think this was all a large misunderstanding, but I'm glad we're on the same page now.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0004.wav"], "content": "Heck, I can barely remember the last time you punched me.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0042.wav"], "content": "Ah ha ha— Wait, should I punch you, then?", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0007.wav"], "content": "N-no! That's not what I meant!", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0004.wav"], "content": "'Cause I'm in GREAT punchin' form right now, and nothing pumps me up like testing my skills against someone!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0056.wav"], "content": "N-no, I'm good. But I'm sure Ranzal would be happy to oblige!", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0074.wav"], "content": "Huh? Hey, wait a second! Come back! There ain't no way I'm fightin' her!", "isSys": false, "speakerIcon": "story/chara/120007_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0002.wav"], "content": "You scared of losing?", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0008.wav"], "content": "Nah, that ain't it.", "isSys": false, "speakerIcon": "story/chara/120007_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0018.wav"], "content": "I wouldn't wanna scar a pretty face. It's against my principles.", "isSys": false, "speakerIcon": "story/chara/120007_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "So I can't fight ya. Sorry!", "isSys": false, "speakerIcon": "story/chara/120007_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0038.wav"], "content": "Whaaaaaat?!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}, {"audioPaths": ["vo_chr_110023_story/VO_CHR_110023_STORY_0056.wav"], "content": "Don't call me pretty, damn it!", "isSys": false, "speakerIcon": "story/chara/110023_01.png", "speakerName": "Johanna", "type": "conversation"}], "id": 110023015, "title": "War Paint (110023015)"}]