[{"conversations": [{"audioPaths": ["vo_chr_120018_story/VO_CHR_120018_STORY_0056.wav"], "content": "Stop! Thief!", "isSys": false, "speakerIcon": "story/chara/120018_01.png", "speakerName": "Woman", "type": "conversation"}, {"audioPaths": ["vo_chr_120032_story/VO_CHR_120032_STORY_0019.wav"], "content": "Ha! Can't catch me now!", "isSys": false, "speakerIcon": null, "speakerName": "Thief", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0050.wav"], "content": "Stay here, Cleo; I'm going after that thief!", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0014.wav"], "content": "I feared you might say that. Just try not to get hurt—", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0042.wav"], "content": "Wait... Who is that?", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_120032_story/VO_CHR_120032_STORY_0002.wav"], "content": "Outta my way, shorty!", "isSys": false, "speakerIcon": null, "speakerName": "Thief", "type": "conversation"}, {"audioPaths": [], "content": "Or I'll MAKE you move!", "isSys": false, "speakerIcon": null, "speakerName": "Thief", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0023.wav"], "content": "I might be short...", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "but that ain't ever stopped me from standin' tall against lowlifes like you!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "You're gonna give back what you stole, one way or another!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120032_story/VO_CHR_120032_STORY_0056.wav", "vo_chr_110033_story/VO_CHR_110033_STORY_0025.wav"], "content": "Hah! I almost feel bad for you, gettin' knocked out after one hit.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0023.wav"], "content": "Shoulda kept your hands to yourself.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0011.wav"], "content": "I believe this is yours, miss.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0030.wav"], "content": "This place ain't the best part of town. Stay safe, yeah?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120018_story/VO_CHR_120018_STORY_0045.wav"], "content": "Thank you... Thank you so much!", "isSys": false, "speakerIcon": "story/chara/120018_01.png", "speakerName": "Woman", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0004.wav"], "content": "Not many people would go out of their way for a stranger in these times.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0012.wav"], "content": "Indeed. It was almost like watching an older brother stick up for his siblings.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_120014_story/VO_CHR_120014_STORY_0042.wav"], "content": "What happened? You out here bein' a hero again, Linus?", "isSys": false, "speakerIcon": "story/chara/120014_01.png", "speakerName": "Resident A", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0025.wav"], "content": "Ah, knock it off. I only did what anyone else woulda done.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120019_story/VO_CHR_120019_STORY_0004.wav"], "content": "You're amazing!", "isSys": false, "speakerIcon": "story/chara/120019_01.png", "speakerName": "Resident B", "type": "conversation"}, {"audioPaths": ["vo_chr_120014_story/VO_CHR_120014_STORY_0004.wav"], "content": "Three cheers for Linus!", "isSys": false, "speakerIcon": "story/chara/120014_01.png", "speakerName": "Resident A", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0074.wav"], "content": "Come on, I was just—", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120019_story/VO_CHR_120019_STORY_0011.wav"], "content": "Hip hip...hooray! Hip hip...hooray!", "isSys": false, "speakerIcon": "story/chara/120019_01.png", "speakerName": "Resident B", "type": "conversation"}, {"audioPaths": ["vo_chr_120042_story/VO_CHR_120042_STORY_0005.wav"], "content": "What would we ever do without you?!", "isSys": false, "speakerIcon": "story/chara/120042_01.png", "speakerName": "Resident C", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0034.wav"], "content": "Knock it off already!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0025.wav"], "content": "It ain't a big deal, so stop actin' like I'm a freakin' saint!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "S-sorry, Linus...", "isSys": false, "speakerIcon": null, "speakerName": "Residents", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0023.wav"], "content": "*sigh* Anyway...", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0030.wav"], "content": "I'm just glad everyone's safe and sound.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120019_story/VO_CHR_120019_STORY_0004.wav"], "content": "See! You really are the best!", "isSys": false, "speakerIcon": null, "speakerName": "Residents", "type": "conversation"}, {"audioPaths": ["vo_chr_120019_story/VO_CHR_120019_STORY_0005.wav"], "content": "Thank you, Linus!", "isSys": false, "speakerIcon": null, "speakerName": "Residents", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0038.wav"], "content": "Didn't you hear me the first time?! I said knock it... Huh?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0008.wav"], "content": "Hey, I know you! You're the ones from that castle, ain't ya?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0011.wav"], "content": "That's right. And if you don't mind my saying, you handled that thief impressively.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0025.wav"], "content": "Eh, if you say so.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "But forget about that. I got somethin' I been meanin' to ask you for a while now.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0042.wav"], "content": "Huh? What's that?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0011.wav"], "content": "See, I'm kinda this town's guardian, but things have been gettin' more and more dangerous around here.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0008.wav"], "content": "So what I'm thinkin' is, I'll lend you my strength, if you'd return the favor and help me take care of the lowlifes that keep botherin' this town. How 'bout it?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0032.wav"], "content": "Well, we could certainly use someone like you on our side...", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0012.wav"], "content": "All right. You've got yourself a deal.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}], "id": 110033011, "title": "Neighborhood Protector (110033011)"}, {"conversations": [{"audioPaths": ["vo_chr_120015_story/VO_CHR_120015_STORY_0006.wav"], "content": "Oh, are you two out patrolling again today? Must be quite the burden...", "isSys": false, "speakerIcon": null, "speakerName": "Woman", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0011.wav"], "content": "Heh. We're just takin' a stroll. Don't worry about us.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0016.wav"], "content": "You've been going like this since long before I came along.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": [], "content": "It must have been difficult to do alone.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0008.wav"], "content": "Eh, not really.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0030.wav"], "content": "There ain't nothin' but good people livin' in this town.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "For a while, I— Huh?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120011_story/VO_CHR_120011_STORY_0056.wav"], "content": "S-someone! Help! Thief!", "isSys": false, "speakerIcon": null, "speakerName": "Elder", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0023.wav"], "content": "Another one? Ain't they learned  their lesson yet?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0052.wav"], "content": "Come on, Linus!", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0011.wav"], "content": "Right. You circle around from the back, and we'll trap 'em between us!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"type": "break"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0023.wav"], "content": "So that makes two thefts and a brawl in one day. ", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0008.wav"], "content": "Hrmmm... This town's goin' crazy.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0045.wav"], "content": "I'm glad I had you backin' me up, Euden.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0025.wav"], "content": "If you hadn't been there, I probably woulda just keeled over on the spot.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "Honestly, Linus, you're amazing.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0042.wav"], "content": "Huh? Where's this comin' from?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0011.wav"], "content": "You go to all these lengths to help, without anyone ever needing to ask. It's incredible.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": [], "content": "It doesn't matter who you're up against— you fight to protect the people who can't protect themselves. That's special.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0030.wav"], "content": "I'm reminded again of what my father meant when he told me to be my people's shield.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0008.wav"], "content": "Something tells me I still have a ways to go in my training...", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0023.wav"], "content": "What are you talkin' about?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0017.wav"], "content": "You didn't so much as flinch when we were takin' care of business today!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "Besides, you don't always gotta be a ＂shield,＂ you know.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0030.wav"], "content": "Nothin' wrong with runnin' if you gotta.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "I know, but...", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0027.wav"], "content": "You ever lose your nerve, you just stay behind me till you find it again.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0042.wav"], "content": "A shield of my own, huh?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0074.wav"], "content": "D-don't be stupid! That ain't what I meant!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0008.wav"], "content": "I just meant, you know, that I could buy you time to look or whatever!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0029.wav"], "content": "I ain't gonna baby you or nothin', yeah?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0025.wav"], "content": "Look, just... Ah, forget it.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0004.wav"], "content": "Enough talk. Let's go get somethin' to eat.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0011.wav"], "content": "My treat, since you helped me out.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0001.wav"], "content": "...Well? You comin' or what?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0045.wav"], "content": "...Thank you, Linus.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}], "id": 110033012, "title": "A Pair of Shields (110033012)"}, {"conversations": [{"audioPaths": [], "content": "*chomp* *gulp*", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0067.wav"], "content": "Ain't nothin' better than a party at the end of a long day!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0012.wav"], "content": "Ha ha! I like it!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0001.wav"], "content": "Let's eat, drink, and be merry till the break'a dawn!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0023.wav"], "content": "It would behoove you both not to eat too much.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0025.wav"], "content": "You will find no comfort from me should your stomachs start to ache.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0004.wav"], "content": "Bwaaa ha ha! I think we'll take the risk!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0019.wav"], "content": "Once you decide on your path, you gotta have the strength to walk it to the end— no matter where it leads!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0012.wav"], "content": "Couldn't have said it better myself!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0008.wav"], "content": "*sigh* There's no getting through to either of you.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": [], "content": "Ah, don't worry about us. Linus can take it; he's strong. Not as strong as me, though! Bwaaa ha ha ha!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "What? Say that again!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0008.wav"], "content": "I mean, a guy can only be so strong when he's that size.", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "If you were a little bigger, ya might stand a chance against me. ...Or not! Ha!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0054.wav"], "content": "Size ain't got nothin' to do with it! It's what's in your heart that matters!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0010.wav"], "content": "Your spirit! Your courage! That's what makes a strong heart, and a strong heart means a strong body!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0034.wav"], "content": "No, no, ya need muscle for that! Muscle!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0019.wav"], "content": "And I got plenty'a muscle. See? This is what strength looks like! Big and brawny!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0033.wav"], "content": "No! It's what's in your heart!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0002.wav"], "content": "Muscle!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0038.wav"], "content": "Grrrrr...", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0038.wav"], "content": "Hngggh...", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0023.wav"], "content": "...Enough. I refuse to be involved in this idiotic conversation.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0011.wav"], "content": "Cleo! You agree that true strength comes from the heart, yeah?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0033.wav"], "content": "No, no, no! It's muscle! Right?", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0033.wav"], "content": "It comes from your mind.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0035.wav"], "content": "Focus on training your mind, and you will come to understand that this debate is meaningless.", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0042.wav"], "content": "You tryin' to say we ain't smart enough? Well, we're not gonna let smarts beat us!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0012.wav"], "content": "Yeah! All right, Linus, this calls for a night'a fierce debate!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"type": "break"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0004.wav"], "content": "Yeah, absolutely!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0031.wav"], "content": "Yer right! Yer exactly right, Linus!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0008.wav"], "content": "It don't matter how much ya study if ya don't have heart!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": [], "content": "But just heart alone ain't no good.", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0031.wav"], "content": "Right, you gotta have muscle, too. Like you said!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0019.wav"], "content": "You gotta have the muscle to protect the ones you love!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100003_story/VO_CHR_100003_STORY_0019.wav"], "content": "Took the words right outta my mouth! Come on, Linus; we got lots more to talk about!", "isSys": false, "speakerIcon": "story/chara/100003_01.png", "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0001.wav"], "content": "Bring on the debate!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0023.wav"], "content": "*sigh*", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}, {"audioPaths": ["vo_chr_100004_story/VO_CHR_100004_STORY_0025.wav"], "content": "There's no talking sense into them...", "isSys": false, "speakerIcon": "story/chara/100004_01.png", "speakerName": "Cleo", "type": "conversation"}], "id": 110033013, "title": "True Strength (110033013)"}, {"conversations": [{"audioPaths": [], "content": "All right, talk. What'd you call me all the way out here for?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120033_story/VO_CHR_120033_STORY_0012.wav"], "content": "Haw haw! I can't believe he came alone.", "isSys": false, "speakerIcon": "story/chara/120033_01.png", "speakerName": "Ruffian A", "type": "conversation"}, {"audioPaths": [], "content": "I bet he's scared senseless, surrounded by all of us!", "isSys": false, "speakerIcon": "story/chara/120033_01.png", "speakerName": "Ruffian A", "type": "conversation"}, {"audioPaths": ["vo_chr_120032_story/VO_CHR_120032_STORY_0004.wav"], "content": "Why d'you think we set it up like this?", "isSys": false, "speakerIcon": "story/chara/120032_01.png", "speakerName": "Ruffian B", "type": "conversation"}, {"audioPaths": ["vo_chr_120032_story/VO_CHR_120032_STORY_0012.wav"], "content": "I knew he'd come by himself if we threatened to hurt those chumps in that sorry excuse for a town.", "isSys": false, "speakerIcon": "story/chara/120032_01.png", "speakerName": "Ruffian B", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0025.wav"], "content": "What do you want?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0023.wav"], "content": "Don't tell me you need all these big goons just to take on little ol' me.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120033_story/VO_CHR_120033_STORY_0027.wav"], "content": "Haw haw! Straight to business, eh?", "isSys": false, "speakerIcon": "story/chara/120033_01.png", "speakerName": "Ruffian A", "type": "conversation"}, {"audioPaths": ["vo_chr_120033_story/VO_CHR_120033_STORY_0002.wav"], "content": "With you capturing our comrades left and right, our business is in serious danger of dying out!", "isSys": false, "speakerIcon": "story/chara/120033_01.png", "speakerName": "Ruffian A", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0042.wav"], "content": "What business?", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0008.wav"], "content": "Are you the ones responsible for all those thefts?!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0025.wav"], "content": "Then you brought this on yourselves! You're pathetic.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120032_story/VO_CHR_120032_STORY_0002.wav"], "content": "Sh-shut it, shorty!", "isSys": false, "speakerIcon": "story/chara/120032_01.png", "speakerName": "Ruffian B", "type": "conversation"}, {"audioPaths": ["vo_chr_120032_story/VO_CHR_120032_STORY_0042.wav"], "content": "You realize you're outnumbered, right?", "isSys": false, "speakerIcon": "story/chara/120032_01.png", "speakerName": "Ruffian B", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0023.wav"], "content": "...So weak.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120032_story/VO_CHR_120032_STORY_0010.wav"], "content": "What? Say that again, I dare you!", "isSys": false, "speakerIcon": "story/chara/120032_01.png", "speakerName": "Ruffian B", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0015.wav"], "content": "I said you're weak! Weak cowards!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0011.wav"], "content": "Come on, then.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "I may be on my own, and you may be bigger'n me...", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "but I can take you all down with one hand tied behind my back!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120033_story/VO_CHR_120033_STORY_0038.wav"], "content": "Y-you're all bark and no bite!", "isSys": false, "speakerIcon": "story/chara/120033_01.png", "speakerName": "Ruffian A", "type": "conversation"}, {"audioPaths": ["vo_chr_120033_story/VO_CHR_120033_STORY_0055.wav"], "content": "Time to put you down, you mangy mutt—", "isSys": false, "speakerIcon": "story/chara/120033_01.png", "speakerName": "Ruffian A", "type": "conversation"}, {"audioPaths": ["vo_chr_120033_story/VO_CHR_120033_STORY_0056.wav", "vo_chr_100003_story/VO_CHR_100003_STORY_0071.wav"], "content": "Looks like ya could use some help, Linus!", "isSys": false, "speakerIcon": null, "speakerName": "Ranzal", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0006.wav"], "content": "Wh-what are you doin' here?!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0011.wav"], "content": "We heard from some townsfolk that you were in trouble.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": [], "content": "I know you'll probably say you don't need our help, and that's fine.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0031.wav"], "content": "But we won't turn our backs on a friend in need!", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0025.wav"], "content": "What a pain in my rear...", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0030.wav"], "content": "Fine. Just don't get in my way!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0050.wav"], "content": "Now let's cut the chitchat and fight!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0012.wav"], "content": "All right!", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}], "id": 110033014, "title": "A Friend in Need (110033014)"}, {"conversations": [{"audioPaths": ["vo_chr_120015_story/VO_CHR_120015_STORY_0006.wav"], "content": "Oh, out patrolling again, are we? You have our thanks.", "isSys": false, "speakerIcon": null, "speakerName": "Woman", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0074.wav"], "content": "I keep tellin' you, we're just out for a stroll!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0025.wav"], "content": "Keep your thanks to yourself.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120019_story/VO_CHR_120019_STORY_0019.wav"], "content": "Come on, Linus. There's no need to get embarrassed about it.", "isSys": false, "speakerIcon": "story/chara/120019_01.png", "speakerName": "Young Man", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0034.wav"], "content": "Sh-shut up already! And get outta here— you're ruinin' the walk!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0023.wav"], "content": "Honestly? Sometimes I can't stand these people.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0004.wav"], "content": "Ha! I get it. Still, I've noticed everyone in town seems a lot happier lately.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0030.wav"], "content": "Yeah.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "Turns out that bunch'a lowlifes we beat down the other day were the only remainin' members of a gang of thieves.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "That should end the streak of robberies then, right?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0032.wav"], "content": "Yup. And it's all thanks to you helpin' me out.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0045.wav"], "content": "I know I've said it before, but I'm grateful.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "And now...I think it's about time for me to go.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0007.wav"], "content": "What? You're leaving town?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_120019_story/VO_CHR_120019_STORY_0038.wav"], "content": "No! That can't be true!", "isSys": false, "speakerIcon": "story/chara/120019_01.png", "speakerName": "Young Man", "type": "conversation"}, {"audioPaths": ["vo_chr_120015_story/VO_CHR_120015_STORY_0037.wav"], "content": "You can't leave!", "isSys": false, "speakerIcon": null, "speakerName": "Woman", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0011.wav"], "content": "Sorry, but this place just ain't interestin' anymore. Even my walks are a bore.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120019_story/VO_CHR_120019_STORY_0037.wav"], "content": "Don't say that! We still need you—", "isSys": false, "speakerIcon": "story/chara/120019_01.png", "speakerName": "Young Man", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0038.wav"], "content": "Shut up!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0025.wav"], "content": "You can't depend on me forever!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "This is your town! Your home!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "You gotta be able to protect it yourselves!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "......", "isSys": false, "speakerIcon": null, "speakerName": "Residents", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0023.wav"], "content": "My back ain't big enough to carry you all through life.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0030.wav"], "content": "So get off! Stand on your own feet and protect this town!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "And when I come back to visit, you better show me somethin'.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0004.wav"], "content": "I want this place to be GREAT!", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_120019_story/VO_CHR_120019_STORY_0004.wav"], "content": "Linus!", "isSys": false, "speakerIcon": null, "speakerName": "Residents", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0011.wav"], "content": "I'll see you around. Take care.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0008.wav"], "content": "Are you sure about this?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0032.wav"], "content": "Yeah, I'm sure.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "I got somethin' else I gotta do, anyway.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_100001_story/VO_CHR_100001_STORY_0042.wav"], "content": "Which is?", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0028.wav"], "content": "There's this prince that always does everything by himself, see? And he lives in this castle that could use someone strollin' around it every day...", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "Linus, I...", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0011.wav"], "content": "It's my turn now.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": ["vo_chr_110033_story/VO_CHR_110033_STORY_0030.wav"], "content": "Way I see it, I owe you a favor. Besides, friends watch each other's backs.", "isSys": false, "speakerIcon": "story/chara/110033_01.png", "speakerName": "Linus", "type": "conversation"}, {"audioPaths": [], "content": "They certainly do.", "isSys": false, "speakerIcon": "story/chara/100001_01.png", "speakerName": "Euden", "type": "conversation"}], "id": 110033015, "title": "Time for a Change (110033015)"}]